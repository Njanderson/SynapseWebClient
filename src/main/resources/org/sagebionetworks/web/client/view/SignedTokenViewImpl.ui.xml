<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui" 
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">

	<bh:Div>
		<bh:Div addStyleNames="color-line" />
		<g:SimplePanel ui:field="header" />
		<b:Container addStyleNames="min-height-500 margin-top-60">
			<b:Row>
				<b:Column size="XS_12">
					<g:SimplePanel ui:field="synapseAlertContainer" />
					<bh:Div ui:field="successUI">
						<b:Alert type="SUCCESS">
							<b:Heading size="H3" ui:field="successMessage" />
						</b:Alert>
					</bh:Div>
				</b:Column>
				<b:Column size="XS_12">
					<b:Button ui:field="okButton" type="PRIMARY" text="Continue" size="LARGE" pull="RIGHT"></b:Button>
				</b:Column>
			</b:Row>
		</b:Container>
		<g:SimplePanel ui:field="footer" />
	</bh:Div>

</ui:UiBinder> 