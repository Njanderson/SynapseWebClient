<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:t="urn:import:org.sagebionetworks.web.client.widget.table.v2"
	xmlns:a="urn:import:org.sagebionetworks.web.client.widget.entity.menu.v2"
	xmlns:bg="urn:import:org.gwtbootstrap3.client.ui.gwt" xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	
    <g:HTMLPanel>
        <b:Row>
            <b:Column size="XS_2">
                <b:Well size="SMALL">
                	<bh:Text ui:field="itemLabel"/>
            	</b:Well>
            </b:Column>
            <b:Column size="XS_8">
                <b:Row ui:field="urlPanel">
                    <b:Column size="XS_5">
                        <b:TextBox ui:field="urlNameField"/>
                    </b:Column>
                    <b:Column size="XS_7">
                        <b:TextBox ui:field="urlAddressField"/>
                    </b:Column>
                </b:Row>
                <b:Row ui:field="entityPanel">
                    <b:Column size="XS_10">
                    	<b:Well size="SMALL">
                    	    <bh:Text ui:field="entityIdField"/>
                    	</b:Well>
                    </b:Column>
                    <b:Column size="XS_2">
                        <b:Button icon="SEARCH" ui:field="entityLookupButton"/>
                    </b:Column>
                </b:Row>
            </b:Column>
            <b:Column size="XS_2">
            	<b:Button icon="REMOVE" ui:field="removeRowButton"/>
	        </b:Column>
       	</b:Row>
        <g:SimplePanel ui:field="entitySearchPanel"/>
    </g:HTMLPanel>
</ui:UiBinder>  
