<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	<g:HTMLPanel>
		<b:Modal ui:field="newThreadModal" title="title" closable="true" fade="true" dataBackdrop="STATIC" dataKeyboard="false">
			<b:ModalBody>
				<bh:Paragraph>You've reached NewThreadModal.</bh:Paragraph>
				<g:FlowPanel ui:field="mainContent" />
			</b:ModalBody>
			<b:ModalFooter>
				<b:Button ui:field="cancelButton" type="DEFAULT" dataDismiss="MODAL">Cancel</b:Button>
				<b:Button ui:field="saveButton" type="PRIMARY">Save</b:Button>
			</b:ModalFooter>
		</b:Modal>
	</g:HTMLPanel>
</ui:UiBinder> 