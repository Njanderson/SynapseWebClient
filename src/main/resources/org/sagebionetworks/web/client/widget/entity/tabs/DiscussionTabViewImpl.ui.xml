<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	<b:Row>
		<bh:Div addStyleNames="margin-left-15 margin-right-15">
			<g:Button ui:field="newThreadButton" styleName="btn btn-lg btn-primary margin-top-10 right" text="New Thread"/>
		</bh:Div>
		<b:Column size="XS_12">
			<g:SimplePanel addStyleNames="margin-top-10" ui:field="discussionContainer"/>
		</b:Column>
	</b:Row>
</ui:UiBinder>
