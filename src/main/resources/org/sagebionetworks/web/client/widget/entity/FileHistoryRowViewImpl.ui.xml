<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:b.select="urn:import:org.gwtbootstrap3.extras.select.client.ui"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table">

	<t:TableRow>
		<t:TableData>
			<g:Anchor ui:field="versionNameLink" />
			<b:FormControlStatic ui:field="versionName" visible="false" />
		</t:TableData>
		<t:TableData>
			<g:SimplePanel ui:field="modifiedByContainer" />
		</t:TableData>
		<t:TableData>
			<b:FormControlStatic ui:field="modifiedOn" />
		</t:TableData>
		<t:TableData>
			<b:FormControlStatic ui:field="size" />
		</t:TableData>
		<t:TableData>
			<g:SimplePanel ui:field="md5LinkContainer" />
		</t:TableData>
		<t:TableData>
			<b:FormControlStatic ui:field="versionComment" />
		</t:TableData>
		<t:TableData>
			<b:Button icon="EDIT" ui:field="editButton" size="SMALL" addStyleNames="displayInline" />
			<b:Button icon="TIMES" ui:field="deleteButton" size="SMALL" addStyleNames="displayInline margin-left-3" />
		</t:TableData>
	</t:TableRow>
</ui:UiBinder>