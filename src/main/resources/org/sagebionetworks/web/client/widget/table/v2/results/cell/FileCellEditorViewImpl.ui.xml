<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table">

	<g:HTMLPanel>
		<b:InputGroup>
			<bh:Text ui:field="idText" >no file</bh:Text>
			<b:Button ui:field="uploadButton" icon="UPLOAD"/>
		</b:InputGroup>

		<b:Modal ui:field="modal" title="Upload File" closable="true"
			fade="true" dataBackdrop="STATIC" dataKeyboard="false" size="SMALL">
			<b:ModalBody>

			</b:ModalBody>
			<b:ModalFooter>
				<b:Button ui:field="saveButton" type="PRIMARY"
					dataLoadingText="Saving...">Save</b:Button>
				<b:Button ui:field="cancelButton" type="DEFAULT"
					dataDismiss="MODAL">Cancel</b:Button>
			</b:ModalFooter>
		</b:Modal>
	</g:HTMLPanel>

</ui:UiBinder>