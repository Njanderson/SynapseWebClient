<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	
	<bh:Div>
		<b:FieldSet>
			<b:FormGroup>
			    
				<b:FormLabel for="teamId">Team ID</b:FormLabel>
				<b:TextBox ui:field="teamIdField" name="teamId" b:id="teamId"/>
				
				<b:FormLabel for="challengeCheckbox">Is this a Challenge Sign-up?</b:FormLabel>
				<b:CheckBox ui:field="isChallengeCheckbox" name="challengeCheckbox" b:id="challengeCheckbox" />
				
				<b:FormLabel for="memberMessageField">Member Message</b:FormLabel>
				<b:TextBox ui:field="memberMessageField" name="memberMessageField" b:id="memberMessageField" />
				
				<b:FormLabel for="joinButtonText">Button Text</b:FormLabel>
				<b:TextBox ui:field="joinButtonField" name="joinButtonText" b:id="joinButtonText" />
				
				<b:FormLabel for="joinSuccessText">Successfully Joined Message</b:FormLabel>
				<b:TextBox ui:field="joinSuccessField" name="joinSuccessText" b:id="joinSuccessText" />
				
				<b:FormLabel for="requestOpenInfoText">Request Open Info Text</b:FormLabel>
				<b:TextBox ui:field="requestOpenInfoField" name="requestOpenInfoText" b:id="requestOpenInfoText" />
				
				<b:FormLabel for="simpleRequestCheckbox">Is this a simple request?</b:FormLabel>
				<b:CheckBox ui:field="isSimpleRequestCheckbox" name="simpleRequestCheckbox" b:id="simpleRequestCheckbox" />
			</b:FormGroup>
		</b:FieldSet>
	</bh:Div>
	
</ui:UiBinder>