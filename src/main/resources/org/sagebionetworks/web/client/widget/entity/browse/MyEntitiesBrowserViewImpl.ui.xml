<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	<bh:Div>
		<!-- Tabs -->
		<b:NavTabs>
			<b:TabListItem active="true" dataTarget="#tab1" text="My Projects"/>
			<b:TabListItem dataTarget="#tab2" text="My Favorites"/>
		</b:NavTabs>
		<b:TabContent>
			<b:TabPane active="true" b:id="tab1" addStyleNames="panel margin-top-15">
				<g:SimplePanel ui:field="myProjectsContainer" />	
			</b:TabPane>
			<b:TabPane b:id="tab2" addStyleNames="panel margin-top-15">
				<g:SimplePanel ui:field="myFavoritesContainer" />
			</b:TabPane>
		</b:TabContent>
	</bh:Div>
	
</ui:UiBinder>